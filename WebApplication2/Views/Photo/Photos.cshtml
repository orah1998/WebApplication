@model WebApplication2.Models.Photo

@{
    ViewBag.Title = "Photos";
}

<h2>Photos</h2>




@*<p>
        @Html.ActionLink("Create New", "Create")
    </p>*@



<table class="table">


    <tr>
        @{

        for (int i = 0; i < Model.list.Count; i++)
        {
            <td>

                <div class="navbar-header">

                    @Html.DisplayFor(modelitem => Model.list[i])
                    <br />
                    <img class="img-rounded" style="border-radius:50%" src=@Model.list[i].ToString() alt="">
                    <br /><br />

                    @Html.DisplayFor(modelitem => Model.listOfDates[i])
                    <br /><br />
                          <center>

                              @Html.ActionLink("View", "View","Photo", new { mip = Model.list[i].ToString() }, new { @class = "btn btn-primary", @width = "10" })   

                              @Html.ActionLink("Delete", "Delete", "Photo", new { mip = Model.list[i].ToString() }, new { @class = "btn btn-danger", @width = "10" })

                          </center>
                </div>
            </td>

    }

        }





    </tr>
</table>
